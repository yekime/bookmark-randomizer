{
    "name": "Bookmark Randomizer",
    "description": "Opens a random page from a Chrome Bookmark Folder",
    "version": "0.1",
    "manifest_version": 3,
    "icons": {
        "128": "images/dice.png"
    },
    "action": {
        "default_title": "Bookmark Randomizer",
        "default_icon": "images/dice.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "bookmarks"
    ]
}